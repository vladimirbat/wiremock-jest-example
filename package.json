{
  "name": "wiremock-example-1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:mock": "concurrently --kill-others --names 'WIREMOCK,JEST' \"npm run wire:silent\" \"npm run test\" ",
    "wire": "wiremock --verbose --port 9999",
    "wire:record": "concurrently --kill-others --names 'WIREMOCK,CONFIG' \"wiremock --verbose --port 9999 --record-mappings --root-dir=wiremock\" \"node ./startRecordWiremock.js\" ",
    "wire:silent": "wiremock --port 9999"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "wiremock": "^2.26.3"
  }
}
